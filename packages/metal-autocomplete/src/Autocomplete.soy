{namespace Autocomplete}

/**
 * This renders the main element.
 * @param? id
 * @param? elementClasses
 * @param? onListItemSelected_
 */
{template .render}
	<div
		class="autocomplete autocomplete-list component {$elementClasses ? ' ' + $elementClasses : ''}"
		data-onclick="handleClick_">
		{call List.render}
			{param events: ['itemSelected': $onListItemSelected_] /}
			{param id: $id /}
			{param ref: 'list' /}
		{/call}
	</div>
{/template}
